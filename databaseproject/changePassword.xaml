<UserControl x:Class="databaseproject.changePassword"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="151" d:DesignWidth="330">
    <Grid>
        <Label Content="New Password" Height="21" HorizontalAlignment="Left" Margin="26,29,0,0" Name="label1" VerticalAlignment="Top" Width="112" />
        <Label Content="Confirm New Password" Height="21" HorizontalAlignment="Left" Margin="26,56,0,0" Name="label2" VerticalAlignment="Top" Width="112" />
        <PasswordBox Height="19" HorizontalAlignment="Left" Margin="160,29,0,0" Name="change_password" VerticalAlignment="Top" Width="140" />
        <PasswordBox Height="19" HorizontalAlignment="Left" Margin="160,58,0,0" Name="confirm_change_password" VerticalAlignment="Top" Width="140" />
        <Button Content="Change Password" Height="22" HorizontalAlignment="Left" Margin="114,95,0,0" Name="reset_password" VerticalAlignment="Top" Width="91" Click="reset_password_Click" />
    </Grid>
</UserControl>
